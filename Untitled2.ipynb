    "# Bước 1: Import thư viện turtle\n",
    "import turtle\n",
    "\n",
    "# Bước 2:\n",
    "t = turtle.Turtle()\n",
    "t.shape(\"turtle\")\n",
    "t.pensize(5)\n",
    "t.pencolor(\"green\")\n",
    "t.speed(1)\n",
    "t.fillcolor(\"red\")\n",
    "# Bước 3: Vẽ\n",
    "t.begin_fill()\n",
    "t.circle(150)\n",
    "# Bước 4: Hoàn thành vẽ\n",
    "turtle.done()"
   ]
  }
